{
    "name": "hostmoz/blade-bootstrap-components",
    "description": "A set of Blade Components based on Boostrap-4 CSS Framework",
    "keywords": [
        "backstageel",
        "blade-bootstrap-components",
        "Laravel",
        "Blade",
        "Bootstrap",
        "Hostmoz"
    ],
    "homepage": "https://github.com/hostmoz/blade-bootstrap-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ElÃ­sio Leonardo",
            "email": "elisio.leonardo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": ">=9.0",
        "livewire/livewire": "^3.2.3",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Hostmoz\\BladeBootstrapComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hostmoz\\BladeBootstrapComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hostmoz\\BladeBootstrapComponents\\BladeBootstrapComponentsServiceProvider"
            ],
            "aliases": {
                "BladeBootstrapComponents": "Hostmoz\\BladeBootstrapComponents\\BladeBootstrapComponentsFacade"
            }
        }
    }
}
